<table>
    <tr>
        <th>Card</th>
        <th>Amount</th>
        <th>Payee</th>
        <th>Date</th>
    </tr>

        {{#each data}}
            <tr>
                <td>{{this.card}}</td>
                <td>{{ this.amount }}</td>
                <td>{{ this.payee }}</td>
                <td>{{ this.dateBought }}</td>
            </tr>
        {{/each}}

</table>
